{
  "name": "usps-components",
  "version": "0.1.0",
  "description": "USPS Design System - Web Components built with Stencil",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/usps-components/usps-components.esm.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "generate": "stencil generate",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "npm run build && storybook build",
    "test": "stencil test --spec"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/core": "^4.41.3"
  },
  "devDependencies": {
    "@stencil/react-output-target": "^0.8.2",
    "@storybook/addon-a11y": "^10.2.3",
    "@storybook/addon-docs": "^10.2.3",
    "@storybook/blocks": "^8.6.14",
    "@storybook/web-components": "^10.2.3",
    "@storybook/web-components-vite": "^10.2.3",
    "@types/jest": "^29.5.14",
    "eslint-plugin-storybook": "10.2.3",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.3.2",
    "storybook": "^10.2.3",
    "vite": "^7.3.1"
  }
}
